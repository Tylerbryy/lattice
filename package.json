{
  "name": "lattice",
  "version": "1.0.0",
  "description": "AI-powered stock analysis CLI that applies Charlie Munger's latticework of 28 mental models through 10 parallel agents. Features live dashboards, DCF modeling, Monte Carlo simulations, and verdicts delivered in Munger's voice.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "lattice": "./bin/lattice.js"
  },
  "scripts": {
    "build": "bun run tsc",
    "dev": "bun run src/index.ts",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "analyze": "bun run src/index.ts analyze",
    "mock": "bun run dist/index.js mock",
    "mock-watch": "bun run dist/index.js mock-watch",
    "watch": "bun run dist/index.js watch"
  },
  "keywords": [
    "stock-analysis",
    "investing",
    "value-investing",
    "charlie-munger",
    "warren-buffett",
    "mental-models",
    "latticework",
    "ai",
    "ai-agents",
    "claude",
    "anthropic",
    "cli",
    "terminal",
    "finviz",
    "dcf",
    "valuation",
    "fundamental-analysis"
  ],
  "author": "Tyler Gibbs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tylerbryy/lattice"
  },
  "homepage": "https://github.com/Tylerbryy/lattice#readme",
  "bugs": {
    "url": "https://github.com/Tylerbryy/lattice/issues"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "exa-js": "^1.0.0",
    "ink": "^4.4.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "marked": "15",
    "marked-terminal": "^7.3.0",
    "react": "^18.2.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
